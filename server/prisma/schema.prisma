generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model LogEntry {
  id        String   @id @default(cuid())
  note      String
  links     String[]
  date      DateTime
  tags      Tag[]    @relation("LogTags")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Tag {
  id    String     @id @default(cuid())
  name  String     @unique
  logs  LogEntry[] @relation("LogTags")
}
